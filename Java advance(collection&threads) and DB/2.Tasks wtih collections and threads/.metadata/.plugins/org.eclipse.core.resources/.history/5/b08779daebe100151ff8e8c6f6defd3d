
public class MailboxException extends Exception {

	private static final long serialVersionUID = -3221031221824729739L;

	public MailboxException() {
		super();
		// TODO Auto-generated constructor stub
	}

	public MailboxException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

	public MailboxException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public MailboxException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public MailboxException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

}
