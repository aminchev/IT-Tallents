package Store;

import java.util.ArrayList;
import java.util.List;

public class Mouse {
	

	public static void main(String[] args) {
		int[] array = {6,9,8};
		List<Integer> list = new ArrayList<>();
		list.add(6);
		list.add(8);
		list.set(1, 9);
		list.remove(0);
		System.out.println(list.toString());
	}
}